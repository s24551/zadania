#include <iostream>
auto init(int a[], int n, int start) -> void
{
     for(int i=0;i<=n;i++)
    {
     a[i-1]=start-1;
     start=start+1;
    }
}
int main()
{
    int rozm,start;
    int a[50000];
    std::cout<<"Podaj rozmiar tablicy: ";
    std::cin>>rozm;
    std::cout<<"Podaj liczbe od ktorej mam zaczac: ";
    std::cin>>start;
   init(a,rozm,start);
   for(int i=0;i<rozm;i++)
   {
       std::cout<<a[i]<<"\n";
   }
return 0;
}
