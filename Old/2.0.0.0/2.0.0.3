#include <iostream>


int main()
{
    int liczba1,liczba2,liczba3;
    std::cout<<"Podaj pierwsza liczbe: ";
    std::cin>>liczba1;
    std::cout<<"Podaj druga liczbe: ";
    std::cin>>liczba2;
    std::cout<<"Podaj trzecia liczbe: ";
    std::cin>>liczba3;
    for(int i=liczba1;i<liczba2;i=i+liczba3)
        std::cout<<i<<"\n";
    if(liczba2<liczba1)
    {
        std::cout<<"Pierwsza liczba jest wieksza od drugiej";
    }
    return 0;
}
