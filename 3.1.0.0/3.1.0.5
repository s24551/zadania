#include <iostream>
#include<time.h>

int amin(int tab[], int n)
{
    int licznik=tab[0];
    for(int i=0;i<n;i++)
    {
       if(tab[i]>licznik)
       {
           licznik=tab[i];
       }
    }
    return licznik;
}
int main()
{
    int rozm;
    int tab[]={42, 9 , -1, 18, 59, 3, 101, 31, 72, 12};
    std::cout<<"Podaj rozmiar tablicy: \n";
    std::cin>>rozm;
    std::cout<<amin(tab, rozm);
    return 0;
}
